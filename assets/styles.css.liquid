.rimage-background {
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;
}
.js .rimage-background.fade-in {
  opacity: 0;
  transition: opacity 500ms;
}
.js .rimage-background.fade-in.lazyloaded {
  opacity: 1;
