/*================ COMPONENTS ================*/
.rimage-outer-wrapper {
  margin-left: auto;
  margin-right: auto;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center center;
}
.rimage-outer-wrapper.fade-in {
  opacity: 0;
  transition: opacity 500ms;
}
.rimage-outer-wrapper.fade-in.lazyloaded {
  opacity: 1;
}
