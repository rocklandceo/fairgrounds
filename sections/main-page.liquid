<div class="row row-space-bottom{% unless template contains 'story' %} reading-width{% endunless %}">
  <h1 class="large-title">{{ page.title }}</h1>
  {%- unless page.content == empty -%}
    <div class="rte clearfix">
      {{ page.content }}
    </div>
  {%- endunless -%}
</div>

{% schema %}
  {
    "name": "Page"
  }
{% endschema %}
